{
  "expo": {
    "name": "LoginWithGoogle",
    "slug": "LoginWithGoogle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ccf",
    "splash": {
      "image": "./assets/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.afonsoingles.googlelogin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.afonsoingles.googlelogin"
    },
    "web": {
      "favicon": "./assets/logos/favicon.png"
    }
  }
}
